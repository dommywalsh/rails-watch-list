<h1><%=@list.name%></h1>

<ul>
<% @bookmarks.each do |bookmark| %>
  <li><h4><%=bookmark.movie.title%></h4></li>
  <h5><%=bookmark.movie.overview%></h5>
  <p><%=bookmark.comment%></p>
  <%= link_to "Delete",
    bookmark_path(bookmark),
    data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}
  %>
<% end %>
</ul>
